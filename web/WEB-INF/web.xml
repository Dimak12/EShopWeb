<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>StockHandler</servlet-name>
        <servlet-class>EShopWeb.StockHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Handler</servlet-name>
        <servlet-class>EShopWeb.StockHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileHandler</servlet-name>
        <servlet-class>EShopWeb.FileHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SellHandler</servlet-name>
        <servlet-class>EShopWeb.SellHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sell</servlet-name>
        <servlet-class>EShopWeb.Sell</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Selling</servlet-name>
        <servlet-class>EShopWeb.Selling</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deduct</servlet-name>
        <servlet-class>EShopWeb.Deduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>EShopWeb.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StockHandler</servlet-name>
        <url-pattern>/StockHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Handler</servlet-name>
        <url-pattern>/Handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileHandler</servlet-name>
        <url-pattern>/FileHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SellHandler</servlet-name>
        <url-pattern>/SellHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sell</servlet-name>
        <url-pattern>/Sell</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Selling</servlet-name>
        <url-pattern>/Selling</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deduct</servlet-name>
        <url-pattern>/Deduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
